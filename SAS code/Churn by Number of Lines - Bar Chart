ods graphics / reset width=6.4in height=4.8in imagemap;

proc sgplot data=WORK.CHURN_NUMERIC (where=(MultipleLines="Yes" OR 
		MultipleLines="No"));
	title height=14pt 
		"Count of Churn vs No Churn in Multiple Lines vs Single Line";
	vbar MultipleLines / group=Churn groupdisplay=cluster datalabel;
	xaxis label="Multiple Lines?";
	yaxis grid label="Count";
run;

ods graphics / reset;
title;
