ods graphics / reset width=6.4in height=4.8in imagemap;

proc sort data=WORK.CHURN_NUMERIC out=_BarChartTaskData;
	by InternetService;
run;

proc sgplot data=_BarChartTaskData (where=(InternetService="Fiber optic"));
	by InternetService;
	vbar InternetService / group=Churn groupdisplay=cluster datalabel stat=percent;
	yaxis grid;
run;

ods graphics / reset;

proc datasets library=WORK noprint;
	delete _BarChartTaskData;
	run;

ods graphics / reset width=6.4in height=4.8in imagemap;

proc sort data=WORK.CHURN_NUMERIC out=_BarChartTaskData;
	by InternetService;
run;

proc sgplot data=_BarChartTaskData (where=(InternetService="No"));
	by InternetService;
	vbar InternetService / group=Churn groupdisplay=cluster datalabel stat=percent;
	yaxis grid;
run;

ods graphics / reset;

proc datasets library=WORK noprint;
	delete _BarChartTaskData;
	run;

ods graphics / reset width=6.4in height=4.8in imagemap;

proc sort data=WORK.CHURN_NUMERIC out=_BarChartTaskData;
	by InternetService;
run;

proc sgplot data=_BarChartTaskData (where=(InternetService="DSL"));
	by InternetService;
	vbar InternetService / group=Churn groupdisplay=cluster datalabel stat=percent;
	yaxis grid;
run;

ods graphics / reset;

proc datasets library=WORK noprint;
	delete _BarChartTaskData;
	run;
